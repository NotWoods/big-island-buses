{
  "name": "big-island-buses",
  "version": "3.0.0",
  "description": "A far easier to use unofficial schedule for the Hawaii Hele-On Bus.",
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@types/google.analytics": "0.0.40",
    "@types/googlemaps": "^3.39.13",
    "@types/jszip": "^3.4.1",
    "html-minifier": "^4.0.0",
    "jszip": "^3.5.0",
    "prettier": "^2.1.1",
    "rollup": "^2.26.11",
    "rollup-plugin-consts": "^1.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "spherical-geometry-js": "^2.0.0",
    "tslib": "^2.0.1",
    "type-fest": "^0.16.0",
    "typescript": "^4.0.2",
    "unistore": "^3.5.2"
  },
  "repository": "github:NotWoods/big-island-buses",
  "author": "Tiger Oakes",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/NotWoods/big-island-buses/issues"
  },
  "homepage": "https://github.com/NotWoods/big-island-buses#readme",
  "scripts": {
    "prebuild": "rollup -c",
    "build": "node lib/api.js google_transit.zip src/site/_data/api.json",
    "postbuild": "eleventy",
    "lint:files": "tsc --noEmit && prettier \"*.{md,css,html}\" \"src/**/*.ts\" rollup.config.js .eleventy.js",
    "lint": "npm run lint:files -- --write",
    "lint:check": "npm run lint:files -- --check"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "proseWrap": "always",
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "printWidth": 120,
          "tabWidth": 2
        }
      }
    ]
  }
}
